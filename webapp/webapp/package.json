{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.11.1",
    "pnpm": "10.11.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:full": "pnpm supabase start & pnpm dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "test:e2e": "dotenvx run --env-file=.env.local -- playwright test -c test/e2e/config/playwright.local.ts",
    "test:e2e:ci": "playwright test -c test/e2e/config/playwright.ci.ts",
    "test:e2e:smoke": "playwright test -c test/e2e/config/playwright.smoke.ts",
    "test:e2e:debug": "pnpm test:e2e --debug",
    "supabase:start": "supabase start --exclude studio,storage-api,supavisor,vector,postgrest,logflare,imgproxy,realtime",
    "supabase:stop": "supabase stop",
    "vercel:pull": "vercel pull --yes --environment=preview",
    "vercel:build": "vercel build",
    "vercel:deploy": "vercel deploy --prebuilt"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.44.1",
    "@headlessui/react": "^2.2.3",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.6",
    "@tanstack/react-query": "^5.76.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.511.0",
    "next": "15.2.5",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-instrument": "^1.7.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.48",
    "@types/pdfkit": "^0.13.9",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "babel-plugin-istanbul": "^7.0.0",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.2.5",
    "eslint-import-resolver-typescript": "^4.3.5",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-instrument": "^6.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-router-mock": "^1.0.2",
    "nyc": "^17.1.0",
    "playwright-test-coverage": "^1.2.12",
    "supabase": "^2.23.4",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3"
  }
}
